
public class Ex19 {
	public static void main(String[] args) {
		int k1 = 7;
		String res = k1 % 2 == 1 ? "홀수" : "짝수";
		System.out.println(res);
		
		
		int k2 = 76;
		res = k1 > 60  ? "합격" : "불합격";
		System.out.println(res);		
		
		
		char k3 = 'H';
		res = k3 >= 'A' && k3 <= 'Z' ? "대문자" : "소문자";
		
		int time = 10;
		int std = time * 8590;
		double over = time > 8 ? (time - 8) * 1.5 * 8590 : 0; 
		
		System.out.println("오늘 일당은 "  + (std + (int)over) + "원 입니다.");
		
	}
	
}
