package com.ict.edu;

public class Ex03 {
	public static void main(String[] args) {
		
		// 합,불
		
		int k1 = 59;
		String str = "불합격";
		
		// 홀,짝 
		
		int k2 = 59;
		str = "짝수";
		
		// 대문자 소문자
		
		char k3 = '1';
		str = "초기화";
		
		// 남, 녀
		
		int k4 = 1;
		
		// 단가
		int time = 10;
		int dan = 8590;
		int pay = 0;
		
		// 두수중 큰 값 
		
		int k7 = 10;
		int k8 = 15;
		int result; 
		
	}
}
