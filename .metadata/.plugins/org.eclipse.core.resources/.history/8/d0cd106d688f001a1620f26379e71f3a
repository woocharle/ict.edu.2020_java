
public class Ex19 {
	public static void main(String[] args) {
		int k1 = 7;
		String res = k1 % 2 == 1 ? "홀수" : "짝수";
		System.out.println(res);
		
		
		int k2 = 76;
		res = k1 > 60  ? "합격" : "불합격";
		System.out.println(res);		
		
		
		char k3 = 'H';
		res = k3 >= 'A' && k3 <= 'Z' ? "대문자" : "소문자";
	}
}
