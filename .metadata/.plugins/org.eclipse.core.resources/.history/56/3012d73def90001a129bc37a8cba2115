package com.ict.edu;

public class Ex07 {
	public static void main(String[] args) {
		System.out.println("while문");
	
		System.out.println("---------------- for vs while ----------------");
		
		System.out.println("for문 이용");
		for (int i = 0; i < 11; i++) {
			System.out.print(i + " ");
		}
		
		System.out.println();
		
		System.out.println("while문 이용");
		int k1 = 0;
		
		while (k1 < 11) {
			System.out.print(k1 + " ");
			k1 += 1;
		}
		
		
		
		
	}
}
