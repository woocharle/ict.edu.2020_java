import java.io.*;
import java.util.*;

public class Main{
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		int end;
		String str_t = ""; 
		
		do {
			String str = br.readLine();
			int input = Integer.parseInt(str);
			end = input;
			if (end != 0){
				str_t = str_t + str + "/";
			}
			
		} while(end != 0);
		
		String[] data = str_t.split("/");
		int[] num = new int[data.length];
				
		// 에라토스테네스의 체
		int[] chk = new int[max + 1];
			
		for(int i = 0; i < chk.length ; i++) {
			if(i < 2) {
				chk[i] = 0;
			}
			else {
				chk[i] = i;
			}
		}
		
		for(int i = 2; i < chk.length; i++) {
			for(int k = 2 * i ; k < chk.length; k += i) {
				chk[k] = 0;
			}
		}
		// 거르기 완료
		
	}

}

