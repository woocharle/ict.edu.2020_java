package com.ict.edu;

public class Ex08 {
	public static void main(String[] args) {
		//중합 while 문
		for (int i = 1; i < 4; i++) {
			for (int j = 1; j < 6; j++) {
				System.out.println("i = " + i + ", j= " + j );
			}
		}
		
		int i = 0;
		
		System.out.println("\n while문으로 써보기");
		
		while(i < 4) {
			int j = 1;
			while (j <6) {
				System.out.println("i = " + i + ", j= " + j );
				j++;
			}
			i++;
		}
		
		
		
	}
}
