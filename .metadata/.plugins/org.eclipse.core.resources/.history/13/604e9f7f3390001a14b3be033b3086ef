		for(int i = 1; i < 11; i++) {
			System.out.println(i + " ¹øÂ° hello");
		}
		
		System.out.println("-------------------------------------------------");
		
		for(int i = 0; i < 11; i++) {
			System.out.print(i + " ");
		}
		
		System.out.println("\n-------------------------------------------------");
		
		for(int i = 0; i < 11; i++) {	
			if (i % 2 == 0) {
				System.out.print(i + " ");
			}
			
		}
		
		System.out.println("\n-------------------------------------------------");
		
		for(int i = 0; i <= 5; i++) {	
			System.out.print(i * 2 + " ");
			
		}
		
	}
}
