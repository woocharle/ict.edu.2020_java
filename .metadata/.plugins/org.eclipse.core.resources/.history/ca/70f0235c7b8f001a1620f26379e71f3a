package com.ict.edu;

public class Ex01 {
	public static void main(String[] args) {
		int k1 = 59;
		String str = "불합격";
		if(k1 >= 60) {
			str = "합격";
		}
		
		System.out.println("결과: " + str);
		System.out.println("수고하셨습니다.");
		
		
		// 홀짝 판별
		int k2 = 59;
		str = "짝수";
		if(k2 % 2 == 1) {
			str = "홀수";
		}
		
		System.out.println("결과: " + str);
		System.out.println("수고하셨습니다.");		int k2 = 7;
		
		
		// 대문자 소문자 판별
		
		
	}
}
