package com.ict.edu;

public class Ex03 {
	public static void main(String[] args) {
		
		// 합,불
		
		int k1 = 59;
		String str = "초기화";
		
		// k1의 점수가 90이상이면 A, 80이상이면 B, 70이상이면 C, 나머지 F
		
		if (k1 >= 90) {
			System.out.println("A");
		}
		else if (k1 >= 80) {
			System.out.println("B");
		}
		else if (k1 >= 70) {
			System.out.println("C");
		}
		else {
			System.out.println("F");
		}
		
		// 대문자 소문자
		
		char k2 = '1';
		str = "초기화";
		
		if (k2 >= 'a' && k2 <= 'z') {
			str = "소문자";
		}	
		else if (k2 >= 'A' && k2 <= 'Z') {
			str = "대문자";	
		} 
		else {

		}
		
		
		// 남, 녀
		
		int k4 = 1;
		
		// 단가
		int time = 10;
		int dan = 8590;
		int pay = 0;
		
		// 두수중 큰 값 
		
		int k7 = 10;
		int k8 = 15;
		int result; 
		
	}
}
